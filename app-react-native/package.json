{
	"name": "bms-pap-react-native",
	"version": "1.0.0",
	"scripts": {
		"start": "APP_ENV=development expo start --dev-client",
		"android": "expo run:android",
		"prebuild": "expo prebuild",
		"build-prod-android-bundle": "APP_ENV=production react-native build-android --mode=release",
		"build-prod-apk-expo": "npx eas build -p android --profile production-apk",
		"build-prod-apk-local": "npx eas build -p android --profile production-apk --local",
		"build-staging-apk-expo": "npx eas build -p android --profile staging-apk",
		"build-staging-apk-local": "npx eas build -p android --profile staging-apk --local",
		"build-dev-apk-expo": "npx eas build -p android --profile development-apk",
		"build-dev-apk-local": "npx eas build -p android --profile development-apk --local",
		"ios": "expo run:ios",
		"web": "expo start --web"
	},
	"dependencies": {
		"@expo/config-plugins": "~10.0.0",
		"@expo/prebuild-config": "~9.0.0",
