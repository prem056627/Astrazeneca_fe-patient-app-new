{
  "cli": {
    "version": ">= 2.4.1"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      },
       "ios": {
        "simulator": true
      }
    },
    "stagingInternal": {
      "channel": "staging",
      "android": {
        "buildType": "app-bundle"
      },
      "distribution": "internal",
      "env": {
        "APP_ENV": "staging"
      }
    },
    "preview": {
      "channel": "development",
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      }
    },
    "previewStaging": {
      "ios": {
        "cocoapods": "1.14.0"
      },
      "channel": "staging",
      "distribution": "internal",
      "env": {
        "APP_ENV": "staging"
      }
    },
    "previewProduction": {
      "channel": "production",
      "distribution": "internal",
      "env": {
        "APP_ENV": "production"
      }
    },
    "development-apk": {
      "channel": "development",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "development"
      }
    },
    "staging-apk": {
      "channel": "staging",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "staging"
      }
    },
    "production-apk": {
      "channel": "production",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "production"
      }
    },
    "staging": {
      "ios": {
        "cocoapods": "1.14.0"
      },
      "channel": "staging",
      "env": {
        "APP_ENV": "staging"
      }
    },
    "production-bundle": {
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
